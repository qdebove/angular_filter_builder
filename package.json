{
  "name": "angular-filter-builder",
  "version": "0.1.0",
  "description": "Angular 20 filter builder library agnostique, prête à intégrer vos propres adaptateurs.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "angular",
    "filter-builder",
    "typescript",
    "bootstrap"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --config vite.config.ts --host --port 4200",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "tsc -p tsconfig.json",
    "build:demo": "vite build --config vite.config.ts",
    "build:watch": "tsc -p tsconfig.json --watch",
    "preview": "vite preview --config vite.config.ts --host --port 4173",
    "lint": "npm run typecheck",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo \"No automated tests configured; see docs/tests.md for guidance.\""
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "^7.8.1",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "^7.8.1",
    "@analogjs/vite-plugin-angular": "^18.0.0",
    "http-server": "^14.1.1",
    "typescript": "^5.2.2",
    "sass": "^1.77.8",
    "vite": "^5.3.4",
    "zone.js": "^0.14.4"
  }
}
